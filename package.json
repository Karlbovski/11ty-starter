{
  "name": "11ty-starter",
  "version": "1.0.1",
  "description": "Just what it needs to start an 11ty project.",
  "scripts": {
    "watch:sass": "sass --watch src/assets/sass:public/css",
    "build:sass": "sass src/assets/sass:public/css",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "dev": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*"
  },
  "keywords": [
    "eleventy",
    "ssg",
    "starter"
  ],
  "author": "Stefano Giorgi",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.34.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.29.0"
  }
}
