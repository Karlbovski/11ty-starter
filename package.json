{
  "name": "11ty-starter",
  "version": "0.0.6",
  "description": "Just what I need to start an 11ty project.",
  "scripts": {
    "watch:sass": "sass --watch src/assets/sass:public/css",
    "build:sass": "sass src/assets/sass:public/css",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "dev": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*"
  },
  "keywords": [
    "eleventy",
    "ssg",
    "starter"
  ],
  "author": "Stefano Giorgi",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git://github.com/Karlbovski/11ty-starter"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "dayjs": "^1.11.7",
    "markdown-it": "^14.1.0",
    "markdown-it-emoji": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.58.3"
  }
}
